#include <stdio.h>

#define MAX 100000

// Function to find the first element occurring exactly k times
int findFirstKTimes(int arr[], int n, int k) {
    int freq[MAX] = {0};   // frequency array (assuming elements are <= 100000 in absolute value)

    // Since array can have negative values, use offset
    int offset = MAX / 2; 

    // Count frequency of each element
    for (int i = 0; i < n; i++) {
        freq[arr[i] + offset]++;
    }

    // Traverse again to check first element with exactly k occurrences
    for (int i = 0; i < n; i++) {
        if (freq[arr[i] + offset] == k) {
            return arr[i];
        }
    }

    return -1; // if no element found
}

int main() {
    int n, k;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter array elements: ");
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Enter k: ");
    scanf("%d", &k);

    int result = findFirstKTimes(arr, n, k);
    printf("Output: %d\n", result);

    return 0;
}
