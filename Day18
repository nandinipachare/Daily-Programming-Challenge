#include <stdio.h>
#include <math.h>

int main() {
    long long N;
    scanf("%lld", &N);

    int count = 0;
    long long i;

    for (i = 1; i * i <= N; i++) {
        if (N % i == 0) {
            if (i == N / i)  // perfect square
                count += 1;
            else
                count += 2;
        }
    }

    printf("%d\n", count);
    return 0;
}
